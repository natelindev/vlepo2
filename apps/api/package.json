{
  "name": "api",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "exports": "./src/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf .turbo && rm -rf dist && echo \"{}\" > src/persistedQueries.json",
    "db:clean": "ts-node --transpile-only prisma/seed.ts -c",
    "db:migrate": "prisma migrate deploy",
    "db:migrate-dev": "prisma migrate dev",
    "db:new-migration": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "dev": "nodemon",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "node dist/src/app.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/identity": "2.1.0",
    "@azure/keyvault-secrets": "4.4.0",
    "@azure/storage-blob": "12.11.0",
    "@devoxa/prisma-relay-cursor-connection": "2.2.2",
    "@koa/cors": "3.3.0",
    "@koa/router": "12.0.0",
    "@pothos/core": "3.13.0",
    "@pothos/plugin-prisma": "3.21.2",
    "@pothos/plugin-relay": "3.19.0",
    "@pothos/plugin-scope-auth": "3.11.2",
    "@pothos/plugin-simple-objects": "3.4.0",
    "@pothos/plugin-validation": "3.5.1",
    "@pothos/plugin-with-input": "3.6.0",
    "@prisma/client": "4.2.1",
    "algoliasearch": "4.14.2",
    "apollo-server-core": "3.10.1",
    "apollo-server-koa": "3.10.1",
    "argon2": "0.28.7",
    "crypto-random-string": "5.0.0",
    "date-fns": "2.29.1",
    "dotenv": "16.0.1",
    "grant": "5.4.21",
    "graphql": "16.5.0",
    "graphql-scalars": "1.18.0",
    "helpers": "*",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-session": "6.2.0",
    "loglevel": "1.8.0",
    "mjml": "4.13.0",
    "next-mdx-remote": "4.1.0",
    "node-fetch": "3.2.10",
    "reading-time": "1.5.0",
    "remark": "14.0.2",
    "remark-mdx": "2.1.2",
    "remark-mdx-to-plain-text": "3.0.0",
    "ts-pattern": "4.0.5",
    "uuid": "8.3.2",
    "zod": "3.18.0",
    "zx": "7.0.8"
  },
  "devDependencies": {
    "@faker-js/faker": "7.4.0",
    "@types/koa-bodyparser": "4.3.7",
    "@types/koa-session": "5.10.6",
    "@types/koa__cors": "3.3.0",
    "@types/koa__router": "8.0.11",
    "@types/node": "18.7.1",
    "@types/uuid": "8.3.4",
    "eslint": "8.21.0",
    "eslint-config-custom-server": "*",
    "nodemon": "2.0.19",
    "prisma": "4.2.1",
    "ts-node": "10.9.1",
    "tsconfig": "*",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
