{
  "name": "api",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "exports": "./src/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf .turbo && rm -rf dist && echo \"{}\" > src/persistedQueries.json",
    "db:clean": "ts-node --transpile-only prisma/seed.ts -c",
    "db:migrate": "prisma migrate deploy",
    "db:migrate-dev": "prisma migrate dev",
    "db:new-migration": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "dev": "nodemon",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "node dist/src/app.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/identity": "3.2.1",
    "@azure/keyvault-secrets": "4.7.0",
    "@azure/storage-blob": "12.14.0",
    "@devoxa/prisma-relay-cursor-connection": "2.2.2",
    "@koa/cors": "4.0.0",
    "@koa/router": "12.0.0",
    "@pothos/core": "3.30.0",
    "@pothos/plugin-prisma": "3.51.0",
    "@pothos/plugin-relay": "3.43.0",
    "@pothos/plugin-scope-auth": "3.19.1",
    "@pothos/plugin-simple-objects": "3.6.8",
    "@pothos/plugin-validation": "3.9.1",
    "@pothos/plugin-with-input": "3.10.1",
    "@prisma/client": "4.14.0",
    "algoliasearch": "4.17.0",
    "apollo-server-core": "3.12.0",
    "apollo-server-koa": "3.12.0",
    "argon2": "0.30.3",
    "crypto-random-string": "5.0.0",
    "date-fns": "2.30.0",
    "dotenv": "16.0.3",
    "grant": "5.4.21",
    "graphql": "16.6.0",
    "graphql-scalars": "1.21.3",
    "helpers": "*",
    "koa": "2.14.2",
    "koa-bodyparser": "4.4.0",
    "koa-session": "6.4.0",
    "loglevel": "1.8.1",
    "mjml": "4.14.1",
    "next-mdx-remote": "4.4.1",
    "node-fetch": "3.3.1",
    "reading-time": "1.5.0",
    "remark": "14.0.2",
    "remark-mdx": "2.3.0",
    "remark-mdx-to-plain-text": "3.0.0",
    "ts-pattern": "4.3.0",
    "uuid": "9.0.0",
    "zod": "3.21.4",
    "zx": "7.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.0",
    "@types/koa-bodyparser": "4.3.10",
    "@types/koa-session": "6.4.1",
    "@types/koa__cors": "4.0.0",
    "@types/koa__router": "12.0.0",
    "@types/node": "20.1.4",
    "@types/uuid": "9.0.1",
    "eslint": "8.40.0",
    "eslint-config-custom-server": "*",
    "nodemon": "2.0.22",
    "prisma": "4.14.0",
    "ts-node": "10.9.1",
    "tsconfig": "*",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
